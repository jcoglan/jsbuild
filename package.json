{ "name"            : "jsbuild"
, "description"     : "Build tool for the JS.Package dependency system"
, "homepage"        : "http://github.com/jcoglan/jsbuild"
, "author"          : "James Coglan <jcoglan@gmail.com> (http://jcoglan.com/)"
, "keywords"        : ["build"]
, "license"         : "MIT"

, "version"         : "1.0.0"
, "engines"         : {"node": ">=0.4.0"}
, "bin"             : {"jsbuild": "./bin/jsbuild"}
, "preferGlobal"    : true
, "dependencies"    : {"nopt": ""}
, "devDependencies" : {"jsclass": "4.0.0"}

, "scripts"         : {"build": "cp node_modules/jsclass/src/package.js lib/package.js"}

, "repository"      : { "type"  : "git"
                      , "url"   : "git://github.com/jcoglan/jsbuild.git"
                      }

, "bugs"            : "http://github.com/jcoglan/jsbuild/issues"
}


